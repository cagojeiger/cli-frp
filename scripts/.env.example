# FRP Server Environment Configuration
# Copy this file to .env and customize the values

# Authentication
FRP_AUTH_TOKEN=your_secure_token_here

# Server Configuration
FRP_SUBDOMAIN_HOST=your-domain.com

# Dashboard Configuration (optional)
FRP_DASHBOARD_ENABLED=false
FRP_DASHBOARD_USER=admin
FRP_DASHBOARD_PASSWORD=your_secure_password_here

# SSL Configuration
SSL_ENABLED=false
LETSENCRYPT_EMAIL=your-email@example.com
LETSENCRYPT_DOMAINS=your-domain.com,www.your-domain.com

# Advanced Settings (optional)
FRP_LOG_LEVEL=info
FRP_MAX_POOL_COUNT=10
FRP_HEARTBEAT_TIMEOUT=90
